<article>
		<p>
				<%@data.length.times do |i|%>
					<p>
						Activity Type :: 
							<%=@data[i].type%>
						,Number of Activities ::
							<%=@data[i].cnt%>
				        ,for month ::
							<%=@data[i].month%>
						, year ::
							<%=@data[i].year%>
					</p>
				<%end%>
		</p>

	</article>

