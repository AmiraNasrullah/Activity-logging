<%=@data%>
	<article>
		<p>
				<%@data.length.times do |i|%>
					<p>
						Day 
							<%=@data[i][0]%>
						,Month :
							<%=@data[i][1]%>
				        ,year 
							<%=@data[i][2]%>
						,Activity type 
							<%=@data[i][3]%>
						,Number of Activities
							<%=@data[i][4]%>
					</p>
				<%end%>
		</p>

	</article>
